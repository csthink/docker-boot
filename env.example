# docker-compose 环境变量配置文件
WEB_DIR=./www

# Linux 基础配置
# 容器内时区
TZ=Asia/Shanghai

# Alpine Linxu 软件源
ALPINE_REPOSITORIES=mirrors.aliyun.com

# Nginx
NGINX_VERSION=1.16-alpine
NGINX_HTTP_PORT=80
NGINX_HTTPS_PORT=443
NGINX_CONFIG_DIR=./docker/config/nginx/conf.d
NGINX_CONFIG_FILE=./docker/config/nginx/nginx.conf
NGINX_CERTS_DIR=./docker/config/nginx/certs
NGINX_LOG_DIR=./docker/log/nginx

# Tomcat

# Java

# MySQL
MYSQL_VERSION=8.0.17
MYSQL_PORT=3306
MYSQL_ROOT_PASSWORD=111111
MYSQL_DATA_DIR=./docker/data/mysql
MYSQL_CONFIG_FILE=./docker/config/mysql/my.cnf

# Redis
REDIS_VERSION=5.0.5-alpine
REDIS_PORT=6379
REDIS_CONFIG_FILE=./docker/config/redis/redis.conf

# Node.js

